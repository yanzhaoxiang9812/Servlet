        <servlet-name>requestTest1</servlet-name>
        <url-pattern>/requestTest1</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>requestTest2</servlet-name>
        <servlet-class>com.example.request.requestTest2</servlet-class>
        <load-on-startup>9</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>requestTest2</servlet-name>
        <url-pattern>/requestTest2</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>requestTest3</servlet-name>
        <servlet-class>com.example.request.requestTest3</servlet-class>
        <load-on-startup>9</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>requestTest3</servlet-name>
        <url-pattern>/requestTest3</url-pattern>
    </servlet-mapping>
</web-app>